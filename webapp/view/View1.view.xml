<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="QuickStartApplication.controller.View1">
	<App>
		<pages>
			<Page title="Your Quick Start application is ready!" navButtonPress="onPress">
				<content>
					<VBox direction="Column" id="vbox0" justifyContent="Center" width="100%">
						<items>
							<Label id="label3" text="Hello, This is a sample" textAlign="Center" width="100%"/>
						</items>
					</VBox>
					<Button text="Click Me" width="100px" id="__button0" press="onPress1"/>
					<Table id="table0" items="{/Entity1_Set}" noDataText="Drop column list items here and columns in the area above">
						<items>
							<ColumnListItem counter="0" id="item1">
								<cells>
									<Text id="text5" maxLines="0" text="{Id}"/>
									<Text id="text6" maxLines="0" text="{field1}"/>
									<Text id="text7" maxLines="0" text="{field2}"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column id="column0">
								<header>
									<Label id="label0" text="Id"/>
								</header>
							</Column>
							<Column id="column1">
								<header>
									<Label id="label1" text="field1"/>
								</header>
							</Column>
							<Column id="column2">
								<header>
									<Label id="label2" text="field2"/>
								</header>
							</Column>
						</columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>